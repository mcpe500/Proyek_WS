    "migrate:development": "npx sequelize-cli db:migrate --env development",
    "seed:development": "npx sequelize-cli db:seed:all --env development",
    "rollback:development": "npx sequelize-cli db:migrate:undo:all --env development",
    "seed:undo:development": "npx sequelize-cli db:seed:undo:all --env development",
    "migrate:test": "npx sequelize-cli db:migrate --env test",
    "seed:test": "npx sequelize-cli db:seed:all --env test",
    "rollback:test": "npx sequelize-cli db:migrate:undo:all --env test",
    "seed:undo:test": "npx sequelize-cli db:seed:undo:all --env test",
    "migrate:production": "npx sequelize-cli db:migrate --env production",
    "seed:production": "npx sequelize-cli db:seed:all --env production",
    "rollback:production": "npx sequelize-cli db:migrate:undo:all --env production",
    "seed:undo:production": "npx sequelize-cli db:seed:undo:all --env production",
    "test": "jest --detectOpenHandles"
  },
