{
  "version": 2,
  "builds": [
    { "src": "src/run.ts", "use": "@vercel/node" },
    { "src": "src/index.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "src/run.ts" },
    { "src": "/api/v1/(.*)", "dest": "src/router/router.ts" },
    { "src": "/api-docs/(.*)", "dest": "src/index.ts" }
  ]
}
