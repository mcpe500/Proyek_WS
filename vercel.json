{
  "version": 2,
  "builds": [
    {
      "src": "./output/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api-docs(/.*)?",
      "dest": "./output/index.js"
    },
    {
      "src": "/location",
      "dest": "./output/index.js"
    },
    {
      "src": "/api/v1(/.*)?",
      "dest": "./output/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "./output/index.js"
    }
  ]
}
